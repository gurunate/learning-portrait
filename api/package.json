{
  "name": "@learning-portrait/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev -p 3800",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:up": "docker-compose up -d --build --remove-orphans",
    "db:down": "docker-compose down --remove-orphans",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio --browser none",
    "db:seed": "prisma db seed",
    "generate": "prisma generate",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "bcrypt": "^5.1.1",
    "bunyan": "^1.8.15",
    "bunyan-pretty-colors": "^0.1.7",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "next": "14.1.4",
    "pino": "^8.19.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.12.0"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^10.8.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bunyan": "^1.8.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/swagger-ui-react": "^4.18.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.4",
    "prisma": "^5.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "node prisma/seed/index.mjs"
  }
}
